<!DOCTYPE html>
<html>
<head>
    <title>Exemplo Node.js + MySQL</title>
</head>
<body>
    <h1>Dados do MySQL</h1>
    <table>
        <thead>
            <tr>
                <th>Nome</th>
                <th>Senha</th>
                <th>Email</th>
                <th>Tipo</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(row) { %>
                <tr>
                    <td><%= row.nome %></td>
                    <td><%= row.senha %></td>
                    <td><%= row.email %></td>
                    <td><%= row.type %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
